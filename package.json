{
  "name": "crogger-utils",
  "version": "1.0.4",
  "description": "Axiom logging utility library",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --minify",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "crogger",
    "axiom",
    "logging",
    "logger",
    "utils"
  ],
  "author": "Croni",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1"
  }
}
